body {
	font-family: Verdana;
	text-align: center;
	margin-left: 0px;
	margin-right: 0px;
	margin-top: 10px;
	margin-bottom:5px;
	scrollbar-face-color: #aaaaaa;
	scrollbar-highlight-color: #aaaaaa;
	scrollbar-shadow-color: #aaaaaa;
	scrollbar-3dlight-color: #eeeeee;
	scrollbar-arrow-color: #eeeeee;
	scrollbar-track-color: #e3e3e3;
	scrollbar-darkshadow-color: #ffffff;
}

/* Estilo para encabezado */
.Width600 {
	width: 600px;
	border-collapse: collapse;
	border-spacing: 0;
}
.Width560 {
	width: 560px;
	border-collapse: collapse;
	border-spacing: 0;
}

/* Estilos para tìtulos */
.Title01 {
	background-color:#6C7073;
	color:#FFFFFF;
	font-size:10pt;
	font-weight: bold;
	padding-left: 10;
	padding-right: 5;
}

/* Estilo para celda que contiene el cuerpo del mail */
.ContenMail01 {
	font-size: 7.5pt;
	color: #444444;
	padding-left: 40px;
	background-color: #f7f7f7;
	padding-right: 40px;
}

.ContenMail02 {
	font-size: 7.5pt;
	color: #444444;
	padding-left: 40px;
	background-color: #FFFFFF;
	padding-right: 40px;
}

/* Estilo para los subtítulos que contiene el mail (se usa para separar grupos de ItTit-ItDet) */
.ItSub {
	width:560px;
	height:40px;
	background-color:#f7f7f7;
	vertical-align:top;
	text-align:center;
	font-size:8pt;
	font-weight: bold;
	color:#505050;
}

/* Estilo para los distintos tipos de datos que contiene el mail */
.ItTit {
	width:180px;
	background-color:#f7f7f7;
	vertical-align:top;
	text-align:right;
	font-size:8pt;
	color:#505050;
}

/* Estilo para el contenido de los distintos tipos de datos que contiene el mail */
.ItDet {
	width:380px;
	border-bottom-style:dotted;
	border-bottom-width:1px;
	padding-left:4px;
	padding-right:4px;
	font-size:8pt;
	color:#807F84;
	/*color:#000000;*/
	background-color:#f7f7f7;
	text-align:justify;
}

.ItInput {
	font-family: Verdana;
	font-size: 8pt;
	border: 1px solid #FFFFFF;
	padding-left: 4px;
	padding-right: 4px;
	padding-top: 1px;
	padding-bottom: 1px;
	background-color: #FFFFFF;
	color:#808080;
}

/* Estilo para el boton que accede al detalle del pedido */
.Submit {
	font-size: 10pt;
	color: #FFFFFF;
	font-family: Neo Sans;
	border: 1px solid #C0C0C0;
	padding-left: 4px;
	padding-right: 4px;
	padding-top: 1px;
	padding-bottom: 1px;
	background-color: #808080;
}

/* Estilo para viñeta */
#ñ {
	color: #01A4E7;
	font-weight: bold;
	font-size: 8pt;
}

/* Estilo para celda usada como separador */
.CS40 {
	height: 40px;
}
.CS20 {
	height: 20px;
}

/* Colores */
.Celeste {
	background-color:#1CA3DB;
}

.GrisOscuro {
	background-color:#6C7073;
}
.GrisClaro {
	background-color:#f7f7f7;
}

/* Mails del tipo columnas con datos */

/* Títulos de las columnas */

.ColTitI, .ColTitC, .ColTitD {
	background-color: #807F84;
	height: 20px;
	border: solid #A4A4A4; 
	border-width: 1px; 
	padding-left: 4px; 
	padding-right: 4px; 
	padding-top: 1px; 
	padding-bottom: 1px;
	color: #FFFFFF;
	font-size: 8pt;
	font-weight: bold;
}

.ColTitI {
	text-align: left;
}

.ColTitC {
	text-align: center;
}

.ColTitD {
	text-align: right;
}

/* Detalle de las columnas */

.ColDetI, .ColDetC, .ColDetD  {
	border-left: solid #A4A4A4; 
	border-left-width: 1px; 
	border-right: solid #A4A4A4; 
	border-right-width: 1px; 
	border-bottom: dotted #A4A4A4; 
	border-bottom-width: 1px; 
	padding-left: 4px; 
	padding-right: 4px; 
	padding-top: 1px; 
	padding-bottom: 1px;
	font-size: 8pt;
	color: #807F84;
}

.ColDetI {
	text-align: left;
}

.ColDetC {
	text-align: center;
}

.ColDetD {
	text-align: right;
}

/* Separadores de las columnas */

.CSDet1, .CSDet2 {
	height: 15px;
	border-left: 1px solid #A4A4A4; 
	border-right: 1px solid #A4A4A4; 
	padding-left: 4px; 
	padding-right: 4px; 
	padding-top: 1px; 
	padding-bottom: 1px; 
}

.CSDet2 {
	border-bottom: 1px solid #A4A4A4; 
}

/* Totales de las columnas */

.ColTotalI, .ColTotalC, .ColTotalD  {
	background-color: #807F84;
	height: 15px;
	border: solid #A4A4A4;
	border-width: 1px;
	padding-left: 4px;
	padding-right: 4px;
	padding-top: 1px;
	padding-bottom: 1px;
	color: #FFFFFF;
	font-size: 8pt;
}

.ColTotalI  {
	text-align: left;
}

.ColTotalC  {
	text-align: center;
}

.ColTotalD  {
	text-align: right;
}

/* Pie de la tabla (de columnas) */

.PieTabla {
	background: url("../images/pietabla.jpg") no-repeat;
}

/* Mails Outlook 2007/2010 */
.Width600_GrisClaro {
	width: 600px;
	border-collapse: collapse;
	border-spacing: 0;
	background-color:#f7f7f7;
}
.Width600_GrisOscuro {
	width: 600px;
	border-collapse: collapse;
	border-spacing: 0;
	background-color:#6C7073;
}
.Width560_GrisOscuro {
	width: 560px;
	border-collapse: collapse;
	border-spacing: 0;
	background-color:#6C7073;
}
.CS40_GrisClaro {
	height: 40px;
	background-color:#f7f7f7;
}
.CS20_GrisClaro {
	height: 20px;
	background-color:#f7f7f7;
}


/* Lista Simple */

.headerGris, .headerCeleste {
	color:#FFFFFF;
	font-family: Neo Sans;
	font-size:10pt;
	font-weight: bold;
	padding-left: 10;
	padding-right: 5px;
}

.headerGris {
	background-color:#6C7073;
}

.headerCeleste {
	background-color: #00A4E4;
}

.Titulo {
	font-family: Neo Sans;
	font-size: 10pt;
	color: #444444;
	padding-left: 10px;
	background-color: #f7f7f7;
}

.Conten {
	font-family: Neo Sans;
	color: #000000;
	font-size: 8pt;
	padding-left: 10px;
}